/*按钮*/

.btn {
    margin: 0 30rpx 0 30rpx;
    font-size: 36rpx;
    height: 88rpx;
    line-height: 2.55555556;
}

.button-hover {
    color: rgba(0, 0, 0, 0.6) !important;
    /*background: #dedede!important;*/
    animation: anim-btn-hover 0.6s ease-in-out;
    animation-fill-mode: forwards;
}

@keyframes anim-btn-hover {
    0% {
        opacity: 1;
    }
    100% {
        opacity: 0.6;
    }
}

.btn-primary {
    background-color: #6288d5;
    color: #fff;
    border: 0;
}


/**=====================cell======================**/

.cell{
    position: relative;
    padding-top: 14rpx;
    line-height: 2.6;
    font-size: 34rpx;
    
}
.cell-title{
    padding-left:32rpx;
    font-size: 28rpx;
    color: #888;
}
.cell-list{
    background-color: white;
    border-top: 1rpx solid white;
    border-bottom: 1rpx solid white;
}

.cell-item{
    position: relative;
    display: flex;
    align-items: center;
    width: 100%;
    background-color: #FFFFFF;
     /* border-bottom: 1rpx solid #e5e5e5;  */
     margin-left: 34rpx; 
    /* padding-right: 20rpx; */
     padding-top: 10rpx;
}
