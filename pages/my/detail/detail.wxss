


.cell {
}
.cell-head {
  background: #efeff4;
}
.cell-group {
  margin-top: 20rpx;
  background: #fff;
  border-top: 1rpx solid #e5e5e5;
  border-bottom: 1rpx solid #e5e5e5;
  overflow: hidden;
}

.cell-item {
  display: flex;
  display: -webkit-flex;

  padding: 20rpx;
  /* margin-left: 30rpx; */
  padding-left: 30rpx;
  border-bottom: 1rpx solid #e5e5e5;
  align-items:center;
  background-color: #fff;
}
.cell-item:last-child{
  border-bottom:0;
}
.cell-item-h {
  padding-right: 30rpx;
    align-items:center;
    min-width: 140rpx;
    font-size: 34rpx;
    color: #353535;
}

.cell-item-h image {
  width: 50rpx;
  height: 50rpx;
  display:block;
}

.cell-item-b {
  flex: 1;
  font-size: 34rpx;
  color: #353535;
}
.cell-item-b-a{
  font-size: 20rpx;
  color: #6ab7f2;
   align-items:flex-end;
}
.cell-item-c {
  flex: 1;
  font-size: 34rpx;
  color: #8C8C8C;
}

.cell-item-h i{
  color: #6ab7f2;
  font-size: 40rpx;
}
.cell-item-f{
  display: flex;
    display: -webkit-flex;
        align-items:center;
}
.cell-item-f{
  color: #c7c7cc;
}
.cell-item-f view:first-child{
  width: 68rpx;
  height: 68rpx;
  margin-right: 10rpx;
}
.cell-head-logo{
  background-repeat: no-repeat;
  background-size: 100%;
  display: flex;
  display: -webkit-flex;
  flex-direction: column;
  align-items: center;
  padding-bottom: 10rpx;
}
.logo{
  margin-top: 50rpx;
  width: 220rpx;
  height: 220rpx;
  border: 1rpx solid;
  border-radius: 50%;
  align-items: content;
}
.summary{
  
  font-size: 34rpx;
  color: #8C8C8C;
  width: 100%;
  min-height: 150rpx;
  /* padding: 24rpx 30rpx; */
  overflow: hidden;
}
.page-foot{
  padding-top: 120rpx;
}
.btn{
  margin: 30rpx;
}
.btn-primary{
  background-color: #6288d5;
  color: #fff;
}
.hidden_textarea{
  line-height: 1.2;
}
