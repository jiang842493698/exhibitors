.msg-title {
  display: flex;
  align-items: center;
  margin-bottom: 10rpx;
}

.msg-title .title {
  flex: 1;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  overflow: hidden;
}

.msg-title .date {
  color: #999;
  font-size: 24rpx;
}

.msg-content {
  color: #999;
  font-size: 28rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
}

.msg-strong {
  color: #6287d5;
}

.cell-footer .btn {
  
  border: inherit;
  border-top: 1rpx solid #ededed;
  border-right: 1rpx solid #ededed;
  color: #999;
  font-size: 26rpx;
  height: 70rpx;
  display: flex;
  flex-grow: 1;
  justify-content: center;
  align-items: center;
  margin: inherit;
  border-radius: 0;
  background-color: inherit;
  line-height: inherit;
}

.btn.btn-cancel {
  color: #999;
}

.btn:active {
  opacity: 0.6;
}

.btn.btn-primary {
  color: #57ac79;
}
